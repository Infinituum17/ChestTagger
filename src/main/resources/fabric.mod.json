{
	"schemaVersion": 1,
	"id": "chesttagger",
	"version": "${version}",
	"name": "Chest Tagger",
	"description": "This mod helps you organize the content of your chests by assigning them a name, and without using annoying signs",
	"authors": [
		"Infinituum"
	],
	"contact": {
		"sources": "https://github.com/Infinituum17/ChestTagger"
	},
	"license": "CC0-1.0",
	"icon": "assets/chesttagger/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"infinituum.chesttagger.ChestTagger"
		],
		"client": [
			"infinituum.chesttagger.ChestTaggerClient"
		],
		"fabric-datagen": [
			"infinituum.chesttagger.ChestTaggerDataGeneration"
		]
	},
	"mixins": [
		"chesttagger.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.14.17",
		"minecraft": "~1.19.4",
		"java": ">=17",
		"fabric-api": "*"
	}
}